define const_pi() {
    return 4*a(1);
}

define const_e() {
    return e(1)
}

define sin(x) {
    if (x == s(2)) {
        return 1;
    }

    return s(x);
}

define cos(x) {
    if (x == const_pi()/2) {
        return 0;
    }

    return c(x);
}

define tan(x) {
    if (x == const_pi()/4) {
        return 1;
    }

    if (x == -const_pi()/4) {
        return -1;
    }

    return s(x)/c(x);
}

define cot(x) {
    if (x == const_pi()/4) {
        return 1;
    }

    if (x == -const_pi()/4) {
        return -1;
    }

    return c(x)/s(x);
}

define sec(x) {
    return 1/cos(x);
}

define csc(x) {
    return 1/sin(x);
}

define asin(x) {
    if (x == 1) {
        return const_pi()/2;
    }

    if (x == -1) {
        return -const_pi()/2;
    }

    return a(x/sqrt(1-x^2));
}

define acos(x) {
    if (x == 0) {
        return const_pi()/2;
    }

    if (x == 1) {
        return 0;
    }

    if (x == -1) {
        return const_pi()/1;
    }

    if (x > 0) {
        return a(sqrt(1-x^2)/x);
    }

    return const_pi()/1 - acos(-x);
}

define atan(x) {
    if (x == 1) {
        return const_pi()/4;
    }

    if (x == -1) {
        return -const_pi()/4;
    }

    return a(x);
}

define atan(x) {
    return a(x);
}

define acot(x) {
    return const_pi()/2-atan(x);
}

define sqr(x) {
    return  x^2
}

define rez(x) {
    return 1.0 / x
}

define cubed(x) {
    return x^3
}

define asec(x) {
    if (x >= 1) {
        return a(sqrt(x^2-1));
    }

    return const_pi()/1 - a(sqrt(x^2-1));
}

define acsc(x) {
    if (x == 1) {
        return const_pi()/2;
    }

    if (x == -1) {
        return -const_pi()/2;
    }

    if (x > 1) {
        return a(1/sqrt(x^2-1));
    }

    return -a(1/sqrt(x^2-1));
}

define ln(x) {
    return l(x);
}

define log(a, b) {
    return l(b)/l(a);
}

define lg(x) {
    return log(10, x);
}

define log10(x) {
    return log(10, x);
}

define log2(x) {
    return log(2, x);
}

define pow(a, b) {
    if (scale(b) == 0) {
        return a ^ b;
    }
    return e(b*l(a));
}

define exp10(x) {
    return pow(10, x)
}

define exp(x) {
    return e(x);
}

define sqrt3(x) {
    if (x <= 0) {
        return 0
    }
    return pow(x, 1/3);
}

define bessel(n, x) {
    return j(n,x);
}

define aa(m, n) {
    if (n < 0) {
        return 0;
    }

    v = 1;
    for (i = 0; i < n; i++) {
        v *= (m - i);
    }
    return v;
}

define fac(n) {
    return aa(n, n);
}

define rad(deg) {
    return deg/180*const_pi();
}

define deg(rad) {
    return rad/const_pi()*180;
}

define sind(x) {
    return sin(rad(x));
}

define cosd(x) {
    return cos(rad(x));
}

define tand(x) {
    return tan(rad(x));
}

define asind(x) {
    return deg(asin(x))
}

define acosd(x) {
    return deg(acos(x))
}

define atand(x) {
    return deg(atan(x))
}
